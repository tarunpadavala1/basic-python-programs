day = int(input("enter the day number:"))
if day == 1:
  print("day is sunday")
elif day == 2:
  print("day is monday")
elif day == 3:
  print("day is tuesday")
elif day == 4:
  print("day is wednesday")
elif day == 5:
  print("day is thursday")
elif day == 6:
  print("day is friday")
elif day == 7:
  print("day is saturday")
